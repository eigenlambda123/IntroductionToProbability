     "text": [
      "5! = 120.0\n",
      "6 choose 2 = 15.0\n",
      "200! = inf ?\n"
     ]
    }
   ],
   "source": [
    "# Factoring and (n choose k)\n",
    "from scipy.special import factorial, comb\n",
    "\n",
    "print('5! =', factorial(5))\n",
    "print('6 choose 2 =', comb(6, 2)) # (6 choose 2)\n",
    "print('200! =', factorial(200), '?') # inf means infinite"
   ]
  }
